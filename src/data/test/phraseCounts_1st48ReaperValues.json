[
  { "index": 0, "text": "TITLE", "frames": 45 },
  { "index": 1, "text": "Some Thing We Are", "frames": 172 },
  { "index": 2, "text": "The room remained dark.", "frames": 97 },
  { "index": 3, "text": "There was little", "frames": 35 },
  { "index": 4, "text": "that would benefit", "frames": 24 },
  { "index": 5, "text": "from illumination.", "frames": 56 },
  { "index": 6, "text": "A stone window", "frames": 45 },
  { "index": 7, "text": "without glass.", "frames": 37 },
  { "index": 8, "text": "A square bed", "frames": 37 },
  { "index": 9, "text": "on casters.", "frames": 40 },
  { "index": 10, "text": "A cistern", "frames": 25 },
  { "index": 11, "text": "of leaves.", "frames": 35 },
  { "index": 12, "text": "There had been books,", "frames": 56 },
  { "index": 13, "text": "but now there were none.", "frames": 100 },
  { "index": 14, "text": "She squinted", "frames": 48 },
  { "index": 15, "text": "toward the wall,", "frames": 43 },
  { "index": 16, "text": "holding his tin of matches", "frames": 55 },
  { "index": 17, "text": "in the thick air.", "frames": 44 },
  { "index": 18, "text": "She lit one", "frames": 49 },
  { "index": 19, "text": "and watched", "frames": 22 },
  { "index": 20, "text": "the flare of the sulphur,", "frames": 48 },
  { "index": 21, "text": "then the dying.", "frames": 62 },
  { "index": 22, "text": "He seemed to be", "frames": 51 },
  { "index": 23, "text": "holding something out to her.", "frames": 93 },
  { "index": 24, "text": "She tried to remember", "frames": 49 },
  { "index": 25, "text": "his features", "frames": 32 },
  { "index": 26, "text": "from before when there had been light.", "frames": 101 },
  { "index": 27, "text": "She tried", "frames": 28 },
  { "index": 28, "text": "to remember", "frames": 15 },
  { "index": 29, "text": "her own.", "frames": 48 },
  { "index": 30, "text": "It was no use.", "frames": 69 },
  { "index": 31, "text": "There was the dry sound", "frames": 63 },
  { "index": 32, "text": "of dead crickets rustling", "frames": 64 },
  { "index": 33, "text": "in the wind.", "frames": 106 },
  { "index": 34, "text": "‘Hey, take it,’", "frames": 61 },
  { "index": 35, "text": "His voice was unexpected", "frames": 52 },
  { "index": 36, "text": "in the milky, thickening dark.", "frames": 87 },
  { "index": 37, "text": "It sounded young", "frames": 34 },
  { "index": 38, "text": "and thin.", "frames": 46 },
  { "index": 39, "text": "It made her want", "frames": 47 },
  { "index": 40, "text": "to remember his mouth.", "frames": 56 },
  { "index": 41, "text": "She tried to remember", "frames": 51 },
  { "index": 42, "text": "the wetness.", "frames": 41 },
  { "index": 43, "text": "It was too much.", "frames": 57 },
  { "index": 44, "text": "Her head hurt", "frames": 51 },
  { "index": 45, "text": "and she felt sick.", "frames": 67 },
  { "index": 46, "text": "She reached out", "frames": 46 },
  { "index": 47, "text": "for what he was holding,", "frames": 52 },
  { "index": 48, "text": "but he had put it down.", "frames": 119 },
  { "index": 49, "text": "CREDITS", "frames": 100 }
]
